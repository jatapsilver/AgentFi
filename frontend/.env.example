# Environment Variables Template
# Copy this file to .env and fill in your values

# Coinbase CDP Configuration
VITE_CDP_PROJECT_ID=your_project_id_here

# Backend API base (NestJS)
VITE_BACKEND_API_URL=http://localhost:3000

# n8n Webhook (Assistant)
VITE_WEBHOOK_URL=http://localhost:5678/webhook/replace_with_actual

# JWT Expiration grace (seconds to force refresh before exp)
VITE_JWT_EXP_GRACE=30